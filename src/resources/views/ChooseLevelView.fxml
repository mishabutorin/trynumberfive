      </HBox>
      <AnchorPane prefHeight="77.0" prefWidth="410.0" style="-fx-background-color: white;">
         <children>
            <Label layoutX="122.0" layoutY="23.0" stylesheets="@../styles/ChoosLevelStyle.css" text="Choose Level">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="BackButton" layoutX="160.0" layoutY="338.0" mnemonicParsing="false" onAction="#BackButtonAction" style="-fx-background-color: back;" stylesheets="@../styles/ChoosLevelStyle.css" text="Back" textFill="WHITE">
         <font>
            <Font name="Unispace-Bold" size="28.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
