         <children>
            <Button fx:id="LevelOne" mnemonicParsing="false" onAction="#LevelOneAction" prefHeight="50.0" prefWidth="90.0" style="-fx-background-color: white;" text="1">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="LevelTwo" mnemonicParsing="false" onAction="#LevelTwoAction" prefHeight="50.0" prefWidth="90.0" style="-fx-background-color: white;" text="2">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="LevelThree" mnemonicParsing="false" onAction="#LevelThreeAction" prefHeight="50.0" prefWidth="90.0" style="-fx-background-color: white;" text="3">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="LevelFour" mnemonicParsing="false" onAction="#LevelFourAction" prefHeight="50.0" prefWidth="90.0" style="-fx-background-color: white;" text="4">
